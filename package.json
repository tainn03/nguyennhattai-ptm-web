{
  "name": "tms-web",
  "version": "1.1.2",
  "private": true,
  "scripts": {
    "dev": "run-script-os",
    "dev:nix": "NODE_OPTIONS='--max-old-space-size=6144' next dev",
    "dev:windows": "set NODE_OPTIONS='--max-old-space-size=6144' & next dev --turbo",
    "build": "npm-run-all prisma:format prisma:generate browserslist && next build",
    "build:standalone": "yarn build && sh ./next.build.sh",
    "start": "next start",
    "start:standalone": "cd ./build&& set PORT=3000&& set HOSTNAME=0.0.0.0&& node server.js",
    "lint": "next lint",
    "clean": "shx rm -rf .next",
    "pipeline": "npm-run-all lint build",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "browserslist": "npx browserslist@latest --update-db",
    "random:secret": "node -e \"console.log(require('crypto').randomBytes(32).toString('base64'));\"",
    "seed": "npm-run-all seed:administrativeUnit seed:organizationInitialValue seed:emailTemplate seed:resourceOperation",
    "seed:administrativeUnit": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} -r tsconfig-paths/register src/seed/AdministrativeUnit/index.ts",
    "seed:organizationInitialValue": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} -r tsconfig-paths/register src/seed/OrganizationInitialValue/index.ts",
    "seed:emailTemplate": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} -r tsconfig-paths/register src/seed/EmailTemplate/index.ts",
    "seed:resourceOperation": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} -r tsconfig-paths/register src/seed/ResourceOperation/index.ts"
  },
